{
  "name": "@pet-shop-api/users-handler",
  "root": "packages/users-handler",
  "sourceRoot": "packages/users-handler/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "options": {
        "outputPath": "build/users",
        "main": "packages/users/index.ts",
        "tsConfig": "packages/users/tsconfig.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/users/**/*.ts"],
        "tsConfig": "packages/users/tsconfig.json"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "jest.config.js",
        "passWithNoTests": true,
        "tsConfig": "tsconfig.json"
      }
    }
  },
  "tags": []
}
